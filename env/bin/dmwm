#!/afs/cern.ch/user/o/ogarzonm/dmwmclient/env/bin/python3
# EASY-INSTALL-ENTRY-SCRIPT: 'dmwmclient','console_scripts','dmwm'
__requires__ = 'dmwmclient'
import re
import sys
from pkg_resources import load_entry_point

if __name__ == '__main__':
    sys.argv[0] = re.sub(r'(-script\.pyw?|\.exe)?$', '', sys.argv[0])
    sys.exit(
        load_entry_point('dmwmclient', 'console_scripts', 'dmwm')()
    )
